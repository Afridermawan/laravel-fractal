{
    "name": "spatie/laravel-fractal",
    "description": "A Fractal service provider for Laravel",
    "keywords": [
        "spatie",
        "laravel-fractal",
        "laravel",
        "fractal",
        "lumen",
        "api",
        "transform"
    ],
    "homepage": "https://github.com/spatie/laravel-fractal",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.6|^7.0",
        "illuminate/support": "~5.1.0|~5.2.0|~5.3.0",
        "league/fractal": "^0.14.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.2.*|3.3.*",
        "phpunit/phpunit" : "^5.5",
        "scrutinizer/ocular": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Fractal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Fractal\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
